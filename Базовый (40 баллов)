# 5. Написать 3 примера разных с оператором in

# First model
cars = ['Mercedes','Audi','BMW','Opel','Seat','Fiat']
model_car = input('Enter the model of your car ')
if model_car in cars:
    print('Wellcom to our club')
else:
    print('You can be our member')

# Second model

bici = ('30km/h')
skate = ('20km/h')
skate in bici
print(bool(skate))

# Third model
l = ('1','2','3','4','5','6','7','8','9')

k = ('-1','-2','-3','-4','-5','-6','-7','-8','-9')

m = input('m from -9 to 9 ')
if m in l:
    print('m is positive')
elif m in k:
    print('m is negative')
else:
    print('m is null')

# 6. Написать 3 примера условия if elif else.
contry = ('Ukraine','Spain','Moldavia')
tel_cod = ['+38','+34','+373']
country = input('Enter your country \n')
if country == 'Ukraine':
    print('The phone code of Ukraine is ',tel_cod.index(0))
elif country == Spain:
    print('The phone code of Spain is ',tel_cod.index(1))
elif country == Moldavia:
    print('The phone cod of Moldavia is ',tel_cod.indexI(2))
else:
    print('The phone cod of this country is not found')
